{
  "Python Development Flake": {
    "prefix": "flake-python",
    "body": [
      "{",
      "  inputs = {",
      "    nixpkgs.url = \"github:NixOS/nixpkgs/${1:25.11}\";",
      "    flake-utils.url = \"github:numtide/flake-utils\";",
      "  };",
      "",
      "  outputs = { self, nixpkgs, flake-utils }:",
      "    flake-utils.lib.eachDefaultSystem (system:",
      "      let",
      "        pkgs = import nixpkgs {",
      "          inherit system;",
      "          overlays = [",
      "            ${2:# Add overlays here}",
      "          ];",
      "        };",
      "",
      "      in",
      "      {",
      "        packages = { inherit (pkgs); };",
      "",
      "        devShells.default = pkgs.mkShell {",
      "          buildInputs =",
      "            with pkgs;",
      "            with python${3:313}Packages;",
      "            [",
      "              python",
      "              python-lsp-server",
      "              black",
      "              ${4:uv}",
      "              ${0:# Add more packages here}",
      "            ];",
      "        };",
      "      });",
      "}"
    ],
    "description": "Python development flake with devShell"
  },
  "Basic Flake": {
    "prefix": "flake",
    "body": [
      "{",
      "  inputs = {",
      "    nixpkgs.url = \"github:NixOS/nixpkgs/${1:25.11}\";",
      "    flake-utils.url = \"github:numtide/flake-utils\";",
      "  };",
      "",
      "  outputs = { self, nixpkgs, flake-utils }:",
      "    flake-utils.lib.eachDefaultSystem (system:",
      "      let",
      "        pkgs = import nixpkgs {",
      "          inherit system;",
      "        };",
      "",
      "      in",
      "      {",
      "        packages = {",
      "          ${2:# Add packages here}",
      "        };",
      "",
      "        devShells.default = pkgs.mkShell {",
      "          buildInputs = with pkgs; [",
      "            ${0:# Add packages here}",
      "          ];",
      "        };",
      "      });",
      "}"
    ],
    "description": "Basic Nix flake template"
  }
}
